<local:MainUserControl x:Class="magicedit.UCClasslistManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:magicedit"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:UCClassListCodeEditor x:Name="classListCodeEditor" Visibility="Hidden"/>
        <local:UCClassListEditor x:Name="classListEditor"/>
        
        <Button x:Name="bView" Content="Switch view" HorizontalAlignment="Right" Margin="0,0,25,10" VerticalAlignment="Bottom" Width="75"
                Click="bView_Click"/>
        
    </Grid>
</local:MainUserControl>
