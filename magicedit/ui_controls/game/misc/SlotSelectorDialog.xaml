<Window x:Class="magicedit.SlotSelectorDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:magicedit"
        mc:Ignorable="d"
        Title="Select Slot" Width="268.488" Height="116.33">
    
    <Grid>
        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0">Which slot do you want to move this item to?</TextBlock>
        <ComboBox x:Name="cb" Height="23" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,31,0,0" Width="241"
                  SelectionChanged="cb_SelectionChanged">
            
        </ComboBox>
        <Button x:Name="bMove" Content="Move" HorizontalAlignment="Left" Margin="44,59,0,0" VerticalAlignment="Top" Width="75"
               Click="bMove_Click" />
        <Button x:Name="bCancel" Content="Cancel" HorizontalAlignment="Left" Margin="144,59,0,0" VerticalAlignment="Top" Width="75"
                Click="bCancel_Click"/>
    </Grid>
</Window>
